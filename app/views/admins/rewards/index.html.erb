<h1>Rewards listing page</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Edit Reward</th>
      <th>Delete Reward</th>
    </tr>
  </thead>

  <tbody>
    <% rewards.each do |reward| %>
      <tr>
        <td><%= reward.title %></td>
         <td><%= reward.description %></td>
          <td><%= reward.price %></td>
        <td><%=button_to 'Edit', edit_admins_reward_path(reward), method: :get %></td>
        <td><%=button_to 'Delete',admins_reward_path(reward), method: :delete, data: {confirm: "Are you sure?"}%></td>
    <% end %>
  </tbody>
</table>
      <%=button_to "Create New Reward", new_admins_reward_path , method: :get%>

